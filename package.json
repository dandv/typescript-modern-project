{
  "name": "tsmodules",
  "version": "1.0.0",
  "description": "TypeScript project template to import modules without extension, and modules with default exports that use node built-ins",
  "keywords": [
    "typescript",
    "modules",
    "es modules",
    "esnext",
    "influxdb",
    "apollo-server"
  ],
  "license": "MIT",
  "author": "dandv",
  "type": "module",
  "main": "run.js",
  "scripts": {
    "build": "rm *.js && tsc",
    "start": "node --experimental-specifier-resolution=node --harmony run.js",
    "lint": "eslint *.ts"
  },
  "dependencies": {
    "influx": "^5.5.1"
  },
  "devDependencies": {
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint": "^6.8.0",
    "typescript": "^3.8.3"
  }
}
