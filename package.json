{
  "name": "tsmodules",
  "version": "1.0.0",
  "description": "TypeScript project template to import modules without extension, and modules with default exports that use node built-ins",
  "keywords": [
    "typescript",
    "modules",
    "es modules",
    "esnext",
    "influxdb",
    "apollo-server"
  ],
  "license": "MIT",
  "author": "dandv",
  "type": "module",
  "main": "run.js",
  "scripts": {
    "build": "rm *.js && tsc",
    "start": "node --experimental-specifier-resolution=node --harmony -r source-map-support/register run.js",
    "lint": "eslint *.ts",
    "test": "node --harmony node_modules/.bin/jest"
  },
  "dependencies": {
    "influx": "^5.5.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^25.2.3",
    "source-map-support": "^0.5.16",
    "typescript": "^3.8.3"
  }
}
